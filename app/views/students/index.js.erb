StudentDBUtils.show("#students");
$("#students-list").html('<%= j render(@students) %>');
$("#students .panel-heading a#add-student-trigger").attr('href', '<%= new_klass_section_student_path(@klass, @section) %>')
$("#students .panel-heading a#take-attendence-trigger").attr('href', '<%= new_klass_section_attendance_registry_path(@klass, @section) %>');
StudentDBUtils.hide("#student-details");
$("a.section-item").removeClass("active");
$("a#<%= dom_id(@section) %>").addClass("active");
StudentDBUtils.hide("#attendance");
